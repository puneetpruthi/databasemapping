package org.mybeans.factory.impl;

import org.mybeans.factory.MatchArg;

public class BinaryMatchArg extends MatchArg {
    private String  keyName;
    private MatchOp op;
    private Object  keyValue;

    public BinaryMatchArg(String keyName, MatchOp op, Object keyValue) {
        this.keyName  = keyName;
        this.op       = op;
        this.keyValue = keyValue;
    }

    public String  getKeyName()  { return keyName;  }
    public MatchOp getOp()       { return op;       }
    public Object  getKeyValue() { return keyValue; }
}
